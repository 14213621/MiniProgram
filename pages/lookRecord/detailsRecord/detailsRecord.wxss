    font-size: 12px;
    font-family:iconfont;
    text-align: center;
    flex-direction: column;
    justify-content: space-around;
    display: flex;

}
.stateItem:before{
    font-size: 28px;
}
.option{
    position: relative;
    border-bottom:1rpx solid #e0e0e0;
    font-family:iconfont;
    padding:10px 0;
    margin-left:15px;
    font-size: 14px;
}

.module .option:last-child{
    border-bottom: none;
}
.option:before{
    position: absolute;
    right:15px;
    top:50%;
    width: 14px;
    height:14px;
    margin-top:-7px;
}
.optionTitle{
    font-family:"iconfont";
}
.optionTitle:before{
    font-size: 18px;
    margin-right: 10px;
}
.collect:before{
