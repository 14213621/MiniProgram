.gs-signup {
  width: 100%;
  min-height: 100vh;
  margin: 0 auto;
  background-color: white;
  overflow: hidden;
}
.signup-title {
  width: 100%;
  line-height: 120rpx;
  padding-left: 30rpx;
  background-color: #1aad19;
  color: #fff;
}
.signup-form {
  display: block;
  margin: 0 auto;
  width: 90%;
  padding-top: 30rpx;
  overflow: hidden;
  -webkit-text-fill-color: gold
}
.signup-section {
  display: flex;
  line-height: 70rpx;
  margin-bottom: 30rpx;
  overflow: hidden;
  background-color: fff;
}
.signup-item {
  width: 30%;
  text-align: right;
  padding-right: 10rpx;
  color: #555;
}

.signup-singleline {
  margin-left: 2%;
  width: 80%;
  height: 70rpx;
  line-height: 70rpx;
  padding-left: 10rpx;
  border-radius: 5rpx;
  background-color: papayawhip;
  color: #999;
  -ms-wrap-flow: IE 10;
}
.UseOldRecord
{
   margin-top: 10px;
   width: 100%;
    display: flex;
  flex-direction: column;
  align-items: center;
  box-sizing: border-box;
}
use-oldinfo
{
  width: 100%;
  background-color: black;
  color: white;
}
.signup-multiline {
  margin-left: 2%;
  width: 68%;
  line-height: 65rpx;
  padding-left: 10rpx;
  border-radius: 5rpx;
  background-color: papayawhip;
  color: #999;
}
.signup-radiogroup {
  background-color: pink;
}
.signup-checkboxgroup {
  background-color: pink;
}
.signup-label {
  display: block;
}
.signup-textarea {
  height: 80rpx;
  width: 80%;
  line-height: 60rpx;
}
.signup-btn {
  width: 50%;
  height: 70rpx;
  line-height: 70rpx;
  background-color: yellow;
  opacity: 0.7
}
.button-hover {
  background-color: #eee;
}
.signup-switch {
  height: 67rpx;
}
.signup-slider {
  margin: 0;
  padding-left: 40rpx;
  background-color: #eee;
}

.title{
   text-align: center;
   color: palevioletred;

}


